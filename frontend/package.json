{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "dev:fast": "set NODE_OPTIONS=--max-old-space-size=4096 && next dev --turbo",
    "dev:profile": "next dev --turbo --profile",
    "dev:debug": "set NODE_OPTIONS=--inspect && next dev --turbo",
    "dev:clean": "if exist .next rd /s /q .next && npm run dev",
    "build": "next build",
    "build:analyze": "set ANALYZE=true && next build",
    "build:profile": "next build --profile",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "lint:cache": "eslint --cache .",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "clean": "if exist .next rd /s /q .next && if exist out rd /s /q out && if exist node_modules\\.cache rd /s /q node_modules\\.cache",
    "clean:all": "if exist .next rd /s /q .next && if exist out rd /s /q out && if exist node_modules\\.cache rd /s /q node_modules\\.cache && if exist .swc rd /s /q .swc",
    "deps:check": "npm outdated",
    "deps:update": "npm update",
    "analyze": "npm run build:analyze"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.2.1",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-table": "^8.21.3",
    "@types/js-cookie": "^3.0.6",
    "@types/react-calendar": "^3.9.0",
    "autoprefixer": "^10.4.21",
    "axios": "^1.11.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "critters": "^0.0.23",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.12",
    "js-cookie": "^3.0.5",
    "lucide-react": "^0.542.0",
    "next": "15.5.2",
    "postcss": "^8.5.6",
    "react": "19.1.0",
    "react-calendar": "^6.0.0",
    "react-countup": "^6.5.3",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.62.0",
    "react-hot-toast": "^2.6.0",
    "react-intersection-observer": "^9.16.0",
    "recharts": "^3.1.2",
    "swr": "^2.3.6",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^14.0.0",
    "@types/node": "^20.19.12",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "tw-animate-css": "^1.3.8",
    "typescript": "^5"
  }
}
